{
  "name": "kolenka-frontend",
  "version": "0.1.9",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build --mode staging && npm version patch --no-git-tag-version",
    "build-prod": "vue-cli-service build && npm version patch --no-git-tag-version",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@caohenghu/vue-colorpicker": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.1.4",
    "highlight.js": "^9.15.8",
    "lodash": "^4.17.14",
    "logdown": "^3.2.8",
    "moment": "^2.24.0",
    "npm-check-updates": "^3.1.18",
    "prosemirror-state": "^1.2.3",
    "pulse-framework": "^1.11.6",
    "speakingurl": "^14.0.1",
    "spectre.css": "^0.5.8",
    "throttle-debounce": "^2.1.0",
    "tiptap": "^1.23.6",
    "tiptap-commands": "^1.10.9",
    "tiptap-extensions": "^1.24.1",
    "vue": "^2.6.10",
    "vue-content-loading": "^1.6.0",
    "vue-meta": "^2.0.4",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.7",
    "vue-scrollto": "^2.15.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-service": "^3.9.2",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "sass": "^1.22.3",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "no-empty": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Edge 14"
  ]
}
