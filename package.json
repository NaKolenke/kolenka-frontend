{
  "name": "kolenka-frontend",
  "version": "0.1.5",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:prod": "vue-cli-service serve --mode production",
    "build": "vue-cli-service build && npm version patch --no-git-tag-version",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@caohenghu/vue-colorpicker": "^1.1.1",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.1.4",
    "highlight.js": "^9.15.8",
    "moment": "^2.24.0",
    "prosemirror-state": "^1.2.3",
    "pulse-framework": "^1.9.4",
    "speakingurl": "^14.0.1",
    "spectre.css": "^0.5.8",
    "throttle-debounce": "^2.1.0",
    "tiptap": "^1.22.4",
    "tiptap-commands": "^1.10.8",
    "tiptap-extensions": "^1.22.4",
    "vue": "^2.6.10",
    "vue-content-loading": "^1.6.0",
    "vue-meta": "^2.0.3",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.6",
    "vue-scrollto": "^2.15.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.4",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "warn",
      "no-empty": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Edge 14"
  ]
}
